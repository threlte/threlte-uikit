{
  "name": "threlte-uikit",
  "version": "1.1.0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/threlte-uikit.git"
  },
  "bugs": {
    "url": "https://github.com/michealparks/threlte-uikit/issues"
  },
  "homepage": "https://github.com/michealparks/threlte-uikit#readme",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest --browser.name=chromium --browser.headless",
    "prettier": "prettier --check",
    "lint": "svelte-kit sync && prettier --check . && eslint .",
    "format": "prettier --write .",
    "release": "changeset publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "dependencies": {
    "@pmndrs/uikit": "^0.8.20",
    "@preact/signals-core": "^1.10.0"
  },
  "peerDependencies": {
    "@threlte/core": ">=8",
    "svelte": ">=5",
    "three": ">=0.160"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@preact/signals-core": "^1.10.0",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.5",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@threlte/core": "^8.0.5",
    "@threlte/extras": "^9.3.0",
    "@threlte/test": "^0.2.9",
    "@types/eslint": "^9.6.1",
    "@types/three": "^0.177.0",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.2",
    "globals": "^16.2.0",
    "playwright": "^1.53.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "svelte": "^5.34.5",
    "svelte-check": "^4.2.1",
    "three": "^0.177.0",
    "three-inspect": "^0.7.2",
    "tslib": "^2.8.1",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
