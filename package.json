{
  "name": "threlte-uikit",
  "version": "0.5.1",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michealparks/threlte-uikit.git"
  },
  "bugs": {
    "url": "https://github.com/michealparks/threlte-uikit/issues"
  },
  "homepage": "https://github.com/michealparks/threlte-uikit#readme",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest --browser.name=chromium --browser.headless",
    "prettier": "prettier --check",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "release": "changeset publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "dependencies": {
    "@pmndrs/uikit": "^0.8.12",
    "@preact/signals-core": "^1.8.0"
  },
  "peerDependencies": {
    "@threlte/core": ">=8",
    "svelte": ">=5",
    "three": ">=0.160"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@preact/signals-core": "^1.8.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.19.2",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@threlte/core": "^8.0.1",
    "@threlte/extras": "^9.1.0",
    "@threlte/test": "^0.2.9",
    "@types/eslint": "^9.6.1",
    "@types/three": "^0.174.0",
    "@vitest/browser": "^3.0.9",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.2.1",
    "globals": "^16.0.0",
    "playwright": "^1.51.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.9",
    "svelte": "^5.23.1",
    "svelte-check": "^4.1.5",
    "three": "^0.174.0",
    "three-inspect": "^0.7.2",
    "tslib": "^2.8.1",
    "type-fest": "^4.37.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  }
}
